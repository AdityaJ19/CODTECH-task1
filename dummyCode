#include<stdio.h>

void stringConcat(char *string1, char *string2){
    while(*string1 != '\0'){
        string1++;
    }

    while(*string2!= '\0'){
        *string1 = *string2;
        string1++;
        string2++;
    }
    *string1 = '\0';
}

int findSubstring(char string[],char subString[]){
    int string_length=stringLength(string);
    int subString_length=stringLength(subString);
    for(int i=0;i<string_length;i++){
        if(subString[0]==string[i]){
            int start=i;
            int j=0;
            while(j<subString_length && subString[j]==string[j+i]){
                j++;
                }
            if(j==subString_length){
                return start;
                break;
            }
            }
        }
    return -1;
}


int main(){
    char string1[100] = "Hello";
    char string2[100] = "World";
    stringConcat(string1, string2);
 
    printf("concatenating strings are %s", string1);
}
